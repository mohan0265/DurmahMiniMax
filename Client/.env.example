# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration (Required)
VITE_SESSION_ENDPOINT=https://your-backend.onrender.com/api/realtime/session
VITE_API_URL=http://localhost:3001

# Voice Configuration
VITE_ALLOW_ANON_VOICE=true
VITE_REQUIRE_LOGIN=false

# WebSocket Configuration
VITE_WS_URL=ws://localhost:3001

# Development Configuration
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_TEXT_FALLBACK=true
VITE_ENABLE_VOICE_SETTINGS=true
VITE_SHOW_TRANSCRIPTS=true

# UI Configuration
VITE_WIDGET_POSITION=bottom-right
